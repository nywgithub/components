    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "doctoc": "^1.4.0",
    "dumi": "^1.1.40",
    "eslint": "^7",
    "gh-pages": "^3.2.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "less": "^3.10.3",
    "lint-staged": "^13",
    "np": "^7.5.0",
    "plop": "^2",
    "prettier": "^2.8.0",
    "prettier-plugin-packagejson": "^2.3.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rimraf": "^3.0.0",
